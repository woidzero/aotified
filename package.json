{
  "name": "aotified",
  "scripts": {
    "build": "bun build ./src/main.ts --outdir=./dist/ --sourcemap && bunx sass ./src/assets/style.scss ./dist/style.css --no-error-css",
    "watch": "bun build ./src/main.ts --outdir=./dist/ --sourcemap --watch & bunx sass --watch ./src/assets/style.scss:./dist/style.css --no-error-css"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@types/jquery": "^3.5.33",
    "jquery": "^3.7.1",
    "preact": "^10.28.2",
    "preact-render-to-string": "^6.6.5",
    "sass": "^1.97.3"
  }
}