{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",

    "lib": ["ESNext", "dom"],
    "target": "ESNext",
    "module": "Preserve",
    "moduleDetection": "force",
    "allowJs": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true
  }
}
